{
  "name": "wikibase-rest-api-ts",
  "version": "1.0.4",
  "description": "Typescript model and API code for interacting with the REST API of a Wikibase instance (like Wikidata)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "generate": "openapi-generator-cli generate",
    "clean": "rm -rf ./src/generated ./dist",
    "clean:windows": "rmdir /s /q src\\generated dist",
    "fix": "sh fix.sh",
    "build": "tsup",
    "list-versions": "openapi-generator-cli version-manager list"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/openetymologymap/wikibase-rest-api-ts.git"
  },
  "keywords": [
    "wikibase",
    "wikidata",
    "typescript",
    "linked data",
    "rest",
    "api"
  ],
  "author": {
    "name": "Daniele Santini",
    "url": "https://www.dsantini.it"
  },
  "funding": {
    "type": "paypal",
    "url": "https://paypal.me/dsantiniit"
  },
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://gitlab.com/openetymologymap/wikibase-rest-api-ts/issues"
  },
  "homepage": "https://gitlab.com/openetymologymap/wikibase-rest-api-ts#readme",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.0",
    "typescript": "^5.5.3"
  }
